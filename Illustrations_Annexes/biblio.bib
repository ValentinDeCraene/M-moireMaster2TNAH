}

@article{galleronRechercheHaidouksAnnoter2021,
	title = {À la recherche des haïdouks. Annoter les entités spatiales dans des romans roumains du xixe siècle},
	rights = {Les contenus de la revue Humanités numériques sont mis à disposition selon les termes de la Licence Creative Commons Attribution 4.0 International.},
	issn = {2736-2337},
